{
  "parser": "babel-eslint",
  "extends": ["airbnb", "plugin:prettier/recommended", "prettier/react"],
  "env": {
    "browser": true,
    "node": true
  },
  "rules": {
    "linebreak-style": ["error", "windows"],
    "react/jsx-filename-extension": "off",
    "react/prop-types": [2, { "ignore": ["children"] }]
  }
}
